{
  "jspm": {
    "directories": {
      "baseURL": "src/static",
      "packages": "src/static/js/vendor/jspm_packages"
    },
    "configFile": "src/static/js/config.js",
    "dependencies": {
      "fetch": "npm:whatwg-fetch@^0.9.0",
      "react": "npm:react@^0.13.3"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  },
  "devDependencies": {
    "jspm": "^0.16.10",
    "node-sass": "^3.3.3",
    "nodemon": "^1.7.1"
  },
  "scripts": {
    "build-js": "jspm bundle-sfx ./src/static/js/app/survey ./src/static/js/out.js",
    "build-css": "node-sass --include-path scss src/static/scss/global.scss src/static/css/global.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\""
  }
}
