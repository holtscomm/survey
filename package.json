{
  "dependencies": {
    "fetch": "^1.1.0",
    "jquery": "^3.1.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-jest": "^15.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "es6-promise": "^4.0.4",
    "exports-loader": "^0.6.3",
    "imports-loader": "^0.6.5",
    "jest": "^15.1.1",
    "jest-cli": "^15.1.1",
    "json-loader": "^0.5.4",
    "node-sass": "^3.3.3",
    "nodemon": "^1.7.1",
    "react-addons-test-utils": "^15.3.2",
    "webpack": "^1.13.2",
    "webpack-dashboard": "^0.2.0"
  },
  "scripts": {
    "preinstall": "bower install",
    "postinstall": "npm run build-css && npm run build-js",
    "build-js": "webpack",
    "watch-js": "webpack-dashboard -- webpack --watch",
    "build-css": "node-sass --include-path scss src/static/scss/main.scss src/static/css/global.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "test": "BABEL_JEST_STAGE=0 jest",
    "build-js:prod": "webpack -p",
    "build-css:prod": "node-sass --include-path scss build/static/scss/main.scss build/static/css/global.css",
    "build:prod": "npm run clean && cp -r src build && export NODE_ENV=production && npm run build-js:prod && npm run build-css:prod",
    "clean": "rm -rf build",
    "deploy": "gcloud app deploy --project=keith-walker-survey build/app.yaml",
    "bnd": "npm run build:prod && npm run deploy",
    "start": "dev_appserver.py src/app.yaml --port=8079 --admin_port=9079 --storage_path=./data/"
  },
  "jest": {
    "scriptPreprocessor": "node_modules/babel-jest",
    "testPathDirs": [
      "src/static/js/app"
    ],
    "unmockedModulePathPatterns": [
      "node_modules/react",
      "node_modules/react-dom"
    ]
  }
}
